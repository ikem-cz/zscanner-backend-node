{
  "name": "zscanner-backend",
  "description": "zScanner backend server. For or extend for your particular needs.",
  "version": "0.9.17",
  "main": "out/src/app.js",
  "typings": "out/src/app.d.ts",
  "author": "Institute for Clinical and Experimental Medicine",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build-ts": "rm -rf out && npx tsc",
    "start": "npm run serve",
    "serve": "node out/src/index",
    "prettier": "prettier ./{src,test,types}/**/*.ts",
    "prettier-fix": "npm run prettier -- --write",
    "tslint-fix": "npx tslint --project . --fix",
    "tslint": "npx tslint --project .",
    "ts-start": "npx ts-node src/index",
    "ts-demo": "env ZSCANNER_AUTHENTICATOR=noop ZSCANNER_STORAGE=demo DEBUG=tus-node-server npx ts-node src/index",
    "test": "npx jest --coverage",
    "prepare": "npm run build-ts"
  },
  "lint-staged": {
    "*.ts": [
      "npm run prettier-fix",
      "npm run tslint-fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "files": [
    "out/",
    "types/"
  ],
  "dependencies": {
    "cron": "^1.8.2",
    "debug": "^2.6.3",
    "express-to-koa": "^2.0.0",
    "fresh": "^0.5.2",
    "handlebars": "^4.7.6",
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.1",
    "koa2-formidable": "^1.0.2",
    "minimist": "^1.2.5",
    "moment": "^2.27.0",
    "pug": "^2.0.0-rc.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "tus-node-server": "git+https://github.com/ljansky/tus-node-server.git",
    "uuid": "^3.4.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cron": "^1.7.2",
    "@types/debug": "^4.1.5",
    "@types/express-to-koa": "^1.0.0",
    "@types/fresh": "^0.5.0",
    "@types/jest": "^26.0.8",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.1",
    "@types/minimist": "^1.2.0",
    "@types/mongoose": "^5.7.34",
    "@types/pug": "^2.0.4",
    "@types/request": "^2.48.5",
    "@types/request-promise": "^4.1.46",
    "@types/supertest": "^2.0.10",
    "@types/tmp": "^0.2.0",
    "@types/uuid": "^8.0.0",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "mockdate": "^3.0.2",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "tmp": "^0.2.1",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.9.7",
    "winston-null": "^2.0.0"
  }
}
